{
  "friendlyName": "Location Content by Geolocation Latency",
  "threshold": {
    "condition": "gte",
    "value": 300
  },
  "applicationName": "Car Engine",
  "environment": "stage",
  "queryConfiguration": {
    "query": "app_name:car_engine AND api:content AND verb:get_geolocation AND tid:hc8mbjncw0",
    "index": "logs-*",
    "windowInSeconds": 300,
    "aggregate": {
      "type": "percentile",
      "field": "time_taken_ms",
      "percentile": 95
    }
  },
  "notifications": {
    "onError": [
      {
        "type": "teams_message",
        "template": "business_latency_failure"
      }
    ],
    "onRecovery": [
      {
        "type": "teams_message",
        "template": "business_latency_recovery"
      }
    ]
  }
}